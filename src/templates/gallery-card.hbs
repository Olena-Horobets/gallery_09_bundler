{{#each this}}
  <li class='gallery__item'>
    <a class='gallery__link' href='{{original}}'>
      <img
        class='gallery__image'
        src='{{preview}}'
        alt='{{description}}'
        data-source='{{original}}'
        data-id='{{@index}}'
        loading='lazy'
      />
    </a>
  </li>
{{/each}}